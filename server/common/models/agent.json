{
  "name": "agent",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "firstLetter": {
      "type": "string"
    },
    "createdAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "chats": {
      "type": "hasMany",
      "model": "chat",
      "foreignKey": ""
    },
    "messages": {
      "type": "hasMany",
      "model": "message",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login",
        "logout",
        "create"
      ]
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "loadAgentsObj"
    }
  ],
  "methods": {}
}
