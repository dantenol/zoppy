{
  "name": "chat",
  "plural": "chats",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "order": "lastMessageAt DESC"
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "customName": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "required": true
    },
    "lastMessageAt": {
      "type": "date",
      "required": true
    },
    "chatId": {
      "type": "string",
      "id": true,
      "required": true
    },
    "pin": {
      "type": "number"
    },
    "mute": {
      "type": "boolean"
    },
    "agentLetter": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "message",
      "foreignKey": ""
    },
    "agent": {
      "type": "belongsTo",
      "model": "agent",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "setup"
    }
  ],
  "methods": {}
}
