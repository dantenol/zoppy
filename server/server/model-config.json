{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "chat": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAll": true,
          "profilePicUrl": true,
          "updateName": true,
          "getMessages": true,
          "markSeen": true,
          "loadMore": true,
          "sendMessage": true,
          "sendAudio": true,
          "checkRecentMessages": true,
          "loadFile": true,
          "uploadMedia": true,
          "claimChat": true,
          "setup": true,
          "loadQR": true,
          "isSet": true,
          "kill": true,
          "forceReloadChat": true,
          "checkNum": true,
          "status": true,
          "reset": true,
          "findChat": true,
          "refocus": true,
          "saveAllConversations": true
        }
      }
    }
  },
  "message": {
    "dataSource": "mongodb",
    "public": false
  },
  "agent": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "login": true,
          "findById": true,
          "logout": true,
          "loadAgentsObj": true,
          "patchAgentData": true,
          "resetPwd": true
        }
      }
    }
  },
  "admin": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "get": true,
          "change": true,
          "version": true
        }
      }
    }
  },
  "sales": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "new": true
        }
      }
    }
  }
}
