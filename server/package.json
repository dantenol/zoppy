{
  "name": "server",
  "version": "v71.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "update-front": "rm -rf ./client/* && cd ../view && npm run build && cp -a ./build/* ../server/client",
    "custom-key": "sed -i 's@openwa.dev@c.zoppy.app@' node_modules/@open-wa/wa-automate/package.json",
    "default-key": "sed -i 's@c.zoppy.app@openwa.dev@' node_modules/@open-wa/wa-automate/package.json",
    "dev-db": "sed -i 's@\"db\"@\"0.0.0.0\"@' ./server/datasources.json",
    "prod-db": "sed -i 's@\"0.0.0.0\"@\"db\"@' ./server/datasources.json"
  },
  "dependencies": {
    "@open-wa/wa-automate": "^3.9.1",
    "aws-sdk": "^2.860.0",
    "axios": "^0.21.1",
    "axios-debug-log": "^0.8.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-fileupload": "^1.2.1",
    "helmet": "^4.4.1",
    "loopback": "^3.28.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.5.1",
    "loopback-connector-mongodb": "^5.5.0",
    "mime-types": "^2.1.29",
    "moment": "^2.29.1",
    "p-queue": "^6.6.2",
    "random": "^3.0.3",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.27.2",
    "socket.io": "^3.1.2",
    "strong-error-handler": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1"
  },
